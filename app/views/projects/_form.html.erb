
<%= form_for (@project) do  |f| %>
<% if  @project.errors.any? %>
	<div id="error_explanation">
		<div class="alert alert-error">
		THe form contains <%= pluralize(@project.errors.count,"error") %>. 
		</div>
		<ul>
		<% @project.errors.full_messages.each do |msg|%>
		<li>* <%= msg%></li>

		<%end%>
		</ul>
		</div>
		<%end%>
	
		<%= f.label :Name %>
		<%= f.text_field :Name %>
	
		
		
		<%= f.label :Filepath %>
		<%= f.text_field :Code%>
		
		
		
		<%= f.label :Description %>
		<%= f.text_area :Description %>
		
		
		
		<%= f.label :Billing_Type%>
		
			<%= f.select :BillingType, options_for_select(%w[TNM Fixed]) %>
		
		
		<%= f.label :Start_Date%>
		<%= f.date_select :StartDate %>
		
		
		
		<%= f.label :DeadLine_Date%>
		<%= f.date_select :DeadlineDate%>
		

		
		<%= f.label :End_Date%>
		<%= f.date_select :EndDate%>
		

		
		<%= f.label :GitHub_URL %>
		<%= f.text_field :gitHubUrl%>
		
		
		
		
		
		<%= f.label :Status %>
		<%= f.select :status, options_for_select(%w[POC Kick_off Completed])%>

	<%= f.select :client_id, Client.all.collect {|u| [u.name, u.id]}, prompt: 'Select' %>
		
		<%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
		<br /><br />
		<% end %>
